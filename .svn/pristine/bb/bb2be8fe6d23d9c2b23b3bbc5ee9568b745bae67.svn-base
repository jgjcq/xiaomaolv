.radius{    border-radius:1.25rem;    width: 4rem;    -moz-box-shadow:1px 1px 4px #B0B0B0;    -webkit-box-shadow:1px 1px 4px #B0B0B0;    box-shadow:1px 1px 4px #B0B0B0;    border:0px solid;    margin: 0.2rem;    color:#878787}.jtkc{    background: url(../images/jingtiaoxixuan.png) no-repeat left center !important;    background-size: 30px 30px !important;    padding-left: 40px !important;}.radius-action{    color:#000000 !important;    -moz-box-shadow:0px 0px 0px #333333;    -webkit-box-shadow:0px 0px 0px #333333;    box-shadow:0px 0px 0px #333333;    background-color: #FAB510 !important;}.center-content{    display: flex;    align-items: center;    justify-content: center;}.kc_list{    display: flex;    justify-content: center;    align-items: center;    overflow: hidden;}.kc_list img{    width: 100%;    border-radius: 0.8rem;    max-width: 18rem;}.kc-title{    padding-top: 0.5rem;    padding-bottom: 0.5rem;}.kc-price{    padding-bottom: 0.5rem;    font-size: 1.2rem;}.kc-after{    border-bottom: 1px solid #d9d9d9;margin-right:4%;    padding-top: 0.5rem;}#search{    border-radius: 50px;    background: #f1f1f1;    letter-spacing: 1px;}.course_type div{    text-align: center;}.course_type_list img{    width: 3rem;}.course_type_list a{    font-size: 0.8rem;    color:#878787}.qkc{    background: url(../images/hkttx.png) no-repeat left center;    background-size: 30px 30px;    padding-left: 40px;}.native-scroll {     padding-top: 34px; }.my-course-list-price{    position: relative;    padding-top: 1rem;    box-sizing: border-box;    display: -webkit-box;    display: -webkit-flex;    display: flex;    -webkit-box-pack: justify;    -webkit-justify-content: space-between;    justify-content: space-between;    -webkit-box-align: center;    -webkit-align-items: center;    align-items: center;    padding-top: 0rem;}.tags-title{    font-size: 0.5rem;    padding-left:1.5rem;}.sq-after{    border-bottom: 1rem solid #d9d9d9;    padding-top: 0.5rem;    margin-left: -4%;    margin-right: -4%;}.searchResultBk {    background: url(../images/暂无搜索内容.png) no-repeat center 120px;    background-size: 50%;}